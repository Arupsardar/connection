<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>/register</title>
  </head>
  <body>
    <div>
      <form action="/register" method="POST">
        <div class="filed">
          <label>Frist Name</label>
          <input type="text" placeholder="Frist Name" name="firstname" id="fristnameinput" />
        </div>
        <div class="filed">
          <label>Last Name</label>
          <input type="text" placeholder="Last Name" name="lastname" id="lastnameinput" />
        </div>
        <div class="filed">
          <label>Email</label>
          <input type="text" placeholder="Email"  name="email" id="Emailinput" />
        </div>
        <div class="filed">
          <label>Gender</label>
          <input type="text" placeholder="Gender" name="email"id="genderinput" />
        </div>
        <div class="filed">
          <label>Password</label>
          <input type="text" placeholder="Password" name="password" id="passwordinput" />
        </div>
        <div class="filed">
          <label>Confirm Password</label>
          <input
            type="text"
            name ="confirmpassword"
            placeholder="confirm Password"
            id="confirmpasswordinput"
          />
        </div>
        <div class="btn">
          <button id="submitbtn">Submit</button>
        </div>

      </form>
    </div>

  </body>
</html>